    <section id="userLog">
      <h2>User Activity Log</h2>
      <table id="userActivityTable">
        <thead>
          <tr>
            <th>Action</th>
            <th>Element</th>
            <th>Timestamp</th>
            <th>Time Difference from Previous</th>
          </tr>
        </thead>
        <tbody>
          <!-- Data rows will go here -->
        </tbody>
      </table>
    </section>
    <script>
        function fetchAndUpdateTable() {
            fetch('/index.js')
            .then(response => response.json())
            .then(data => {
            const tableBody = document.getElementById('userActivityTable').getElementsByTagName('tbody')[0];
            tableBody.innerHTML = ''; // Clear existing rows
    
            // Add new rows from fetched data
            data.forEach(row => {
                let newRow = tableBody.insertRow();
                Object.keys(row).forEach(key => {
                    let cell = newRow.insertCell();
                    let text = document.createTextNode(row[key]);
                    cell.appendChild(text);
                });
            });
            })
            .catch(error => console.error('Error:', error));
        }
    
        // Update the table every 10 seconds (or your preferred interval)
    setInterval(fetchAndUpdateTable, 10000); 
  </script>
    
  
