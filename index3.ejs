    <script>
        document.addEventListener('DOMContentLoaded', function () {
            // Sample data - Replace this with your dynamic data source
            const userActivityData = {
                labels: ["2024-01-01", "2024-01-02", "2024-01-03", "2024-01-04", "2024-01-05"], // Dates
                datasets: [{
                    label: 'Daily User Activities',
                    data: [120, 150, 180, 200, 170], // Number of users per day
                    backgroundColor: 'rgba(54, 162, 235, 0.2)',
                    borderColor: 'rgba(54, 162, 235, 1)',
                    borderWidth: 1
                }]
            };

            // Chart options
            const userActivityOptions = {
                scales: {
                    x: {
                        type: 'category',
                        title: {
                            display: true,
                            text: 'Date'
                        }
                    },
                    y: {
                        beginAtZero: true,
                        title: {
                            display: true,
                            text: 'Number of User Activities'
                        }
                    }
                },
                plugins: {
                    tooltip: {
                        callbacks: {
                            label: (context) => {
                                const label = context.dataset.label || '';
                                const value = context.parsed.y;
                                return ${label}: ${value} activities;
                            }
                        }
                    }
                }
            };

            // Initialize chart
            const ctx = document.getElementById('userActivityChart').getContext('2d');
            const userActivityChart = new Chart(ctx, {
                type: 'bar',
                data: userActivityData,
                options: userActivityOptions
            });
        });
    </script>
